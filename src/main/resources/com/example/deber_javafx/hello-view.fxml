<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="270.0" prefWidth="450.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.deber_javafx.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <Label prefHeight="17.0" prefWidth="98.0" text="Usuario:" />
   <TextField />

  <Label fx:id="welcomeText" prefHeight="17.0" prefWidth="97.0" text="Nombre:" />
   <TextField />
   <Label prefHeight="17.0" prefWidth="93.0" text="Cedula:" />
   <TextField />
   <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="72.0" text="Borrar" />
  <Button nodeOrientation="LEFT_TO_RIGHT" onAction="#onHelloButtonClick" prefHeight="25.0" prefWidth="74.0" text="Actualizar" />
   <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="71.0" text="Insertar" />
</VBox>
